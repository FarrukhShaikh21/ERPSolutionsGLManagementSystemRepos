A.glsub_control_a_c,                    
A.glsubsidary_a_c                    
from VW_GL_Debit a      
where a.LocationId = :P_ADF_LOC_CODE    
AND ((NVL(COA_Type, 'JV') = :P_ADF_VOUCHER_TYPE  AND :P_ADF_GL_VOUCHER_TYPE='R')  OR (NVL(A.COA_Type, 'JV') = 'JV'  AND :P_ADF_GL_VOUCHER_TYPE='P'  AND :P_ADF_VOUCHER_TYPE!='PV') OR (a.glnature_of_a_c = 'E'  AND :P_ADF_VOUCHER_TYPE='PV') OR (NVL(A.COA_Type, 'JV') in('BV', 'CV', 'PV')  AND :P_ADF_VOUCHER_TYPE='CNT'))   
AND A.GL_Code   
NOT IN                
(Select R.gl_code                
From COA_LOC_MAP_Restrict R               
Where R.locationid = :P_ADF_LOC_CODE               
AND R.voucher_type = :P_ADF_VOUCHER_TYPE               
AND R.restrict_side = 'D' AND  :P_ADF_VOUCHER_TYPE!='PV') ORDER BY A.DESCRIPTION]]></SQLQuery>
