  <SQLQuery><![CDATA[with vw_param as(  
                select :P_ADF_VOUCHER_TYPE P_ADF_VOUCHER_TYPE,  
                       :P_ADF_GL_V_TYPE P_ADF_GL_V_TYPE,  
                       :P_ADF_LOC_CODE P_ADF_LOC_CODE  
                 from dual  
                 )        
select A.GL_Code,                
A.DESCRIPTION,            
A.glnature_of_a_c,            
A.glbroad_category,            
A.glcontrol_a_c,              
A.glsub_control_a_c,            
A.glsubsidary_a_c            
from VW_GL_CREDIT A , vw_param  
WHERE A.LocationId = vw_param.P_ADF_LOC_CODE            
AND ((NVL(A.COA_Type, 'JV') = 'JV' AND vw_param.P_ADF_GL_V_TYPE ='R' AND vw_param.P_ADF_VOUCHER_TYPE !='CNT') OR (NVL(A.COA_Type, 'JV') = vw_param.P_ADF_VOUCHER_TYPE AND vw_param.P_ADF_GL_V_TYPE ='P' AND vw_param.P_ADF_VOUCHER_TYPE !='CNT') OR (NVL(A.COA_Type, 'JV') in('BV', 'CV', 'PV') AND vw_param.P_ADF_VOUCHER_TYPE ='CNT'))    
AND A.GL_CODE NOT IN(select UM.ACCOUNT_CODE from coa_user_map UM WHERE UM.USERID=:P_ADF_USERID) 
AND a.GL_Code NOT IN            
(Select R.gl_code                   
From COA_LOC_MAP_Restrict R                   
Where R.locationid = vw_param.P_ADF_LOC_CODE                   
AND R.voucher_type = vw_param.P_ADF_VOUCHER_TYPE            
AND R.restrict_side = 'C') ORDER BY A.DESCRIPTION]]></SQLQuery>
